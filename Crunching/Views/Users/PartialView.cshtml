@model IEnumerable<Crunching.Models.Users>
@{
    Layout = null;
}

<div class="row">
    <div class="col-sm-10 col-sm-offset-1 margin-bottom-xxl ">
        <em class="text-caption">154 days ago</em>
        <div class="card card-underline">
            <div class="card-head">
                <header><i class="fa fa-smile-o margin-right-xl"></i> Contact added</header>
            </div><!--end .card-head -->
            <div class="card-body no-padding-bottom">
                <div class="row">
                    <div class="col-sm-2">
                        <img style="width:100%;" src="@Model.image" />
                    </div>
                    <div class="col-sm-10">
                        <strong>@Model.name, @Model.position + "</strong><br />E-Mail: <a href="mailto:@Model.email">@Model.email</a><br />Phone: @Model.phone<br />
                    </div>
                </div>
            </div><!--end .card-body -->
            <div class="card-actionbar">
                <div class="card-actionbar-row">
                    <a href="javascript:void(0);" class="btn btn-flat btn-default-dark ink-reaction">Contact @Model.name now</a>
                </div>
            </div><!--end .card-actionbar -->
        </div><!--end .card -->
    </div><!--end .col -->
</div><!--end .row -->